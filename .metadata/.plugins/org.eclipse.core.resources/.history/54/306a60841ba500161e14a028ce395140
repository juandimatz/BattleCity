		GolpesResis=3;
		nivel = 4;
		VelMovimiento=0;
		VelDisparo=2;
		DispSim=2; 
		siguiente = null;
		ColeccionDeImagenes=new Icon[4];
		ColeccionDeImagenes[0]=  new ImageIcon(this.getClass().getResource("/Recursos/4JugadorU.gif"));
		ColeccionDeImagenes[1]=  new ImageIcon(this.getClass().getResource("/Recursos/4JugadorD.gif"));
		ColeccionDeImagenes[2]=  new ImageIcon(this.getClass().getResource("/Recursos/4JugadorL.gif"));
		ColeccionDeImagenes[3]=  new ImageIcon(this.getClass().getResource("/Recursos/4JugadorR.gif"));
	}
}
