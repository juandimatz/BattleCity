package Nivel;

import javax.swing.Icon;
import javax.swing.ImageIcon;

public class NivelDos extends Nivel 
{

	public NivelDos()
	{
		GolpesResis=0;
		nivel = 2;
		VelMovimiento=1; 
		VelDisparo=1;
		DispSim=0; 
		siguiente = new NivelTres();
		ColeccionDeImagenes = new Icon[4];
		ColeccionDeImagenes[0]=  new ImageIcon(this.getClass().getResource("/Recursos/2JugadorU.gif"));
		ColeccionDeImagenes[1]=  new ImageIcon(this.getClass().getResource("/Recursos/2JugadorD.gif"));
		ColeccionDeImagenes[2]=  new ImageIcon(this.getClass().getResource("/Recursos/2JugadorL.gif"));
		ColeccionDeImagenes[3]=  new ImageIcon(this.getClass().getResource("/Recursos/2JugadorR.gif"));
	}	
}
